_onFetch(page = 1, callback, options) {
    fetch(API_URL + '?page=' + page)
      .then((response) => response.json())
      .then((responseData) => {
        this.data = responseData.posts;
        page++;
        connError = 0;
        callback(this.data);
      })
      .catch((error) => {
        console.log(error);
        connError = 1;
        callback(null);
      })
    .done();
  },